  "scripts": {
    "build": "SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "preinstall": "npx only-allow pnpm",
    "lbt": "npm run lint && npm run build && npm run test",
    "lint": "eslint --ext .ts --ext tsx src/",
    "lint:fix": "npm run lint -- --fix",
    "start": "PORT=3001 SKIP_PREFLIGHT_CHECK=true react-scripts start",
    "test": "echo No tests"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": "@yellowgarbagegroup/eslint-config",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/react-fontawesome": "0.1.15",
    "@testing-library/dom": "8.1.0",
    "@testing-library/react": "11.2.7",
    "@testing-library/user-event": "13.2.1",
    "@types/react": "17.0.19",
    "@types/react-dom": "17.0.9",
    "@yellowgarbagebag/common-lib": "workspace:*",
    "@yellowgarbagebag/csms-lib": "workspace:*",
    "bootstrap": "4.6.0",
    "dayjs": "1.10.6",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "4.0.3",
    "react-tabs": "3.2.2",
    "typescript": "4.3.5",
    "uuid": "8.3.2",
    "web-vitals": "2.1.0"
  },
  "devDependencies": {
    "@types/node": "14.17.13",
    "@types/react-tabs": "2.3.3",
    "@types/uuid": "8.3.1",
    "@yellowgarbagegroup/eslint-config": "^2.0.1",
    "eslint": "7.32.0",
    "prettier": "2.3.2"
  },
  "engines": {
    "node": ">=14"
  }
