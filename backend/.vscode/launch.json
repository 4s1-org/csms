{
  "version": "0.2.0",
  "configurations": [{
    "name": "Debug Nest Framework",
    "type": "node",
    "request": "launch",
    "cwd": "${workspaceRoot}",
    "runtimeArgs": [
      "--nolazy",
      "-r",
      "ts-node/register",
      "-r",
      "tsconfig-paths/register"
    ],
    "args": ["${workspaceRoot}/src/main.ts"],
    "sourceMaps": true,
    "envFile": "${workspaceFolder}/.env",
    "console": "integratedTerminal",
    "protocol": "inspector"
  }]
}
