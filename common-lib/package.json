{
  "name": "@4s1/common-lib",
  "version": "1.0.0",
  "private": true,
  "description": "Common Library",
  "bugs": {
    "url": "https://gitlab.com/4s1/csms/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/4s1/csms.git",
    "directory": "common-lib"
  },
  "license": "MIT",
  "author": "Steffen <steffen@4s1.de>",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc",
    "build:watch": "tsc --watch",
    "format": "prettier --write \"src/**/*.ts\" \"scripts/**/*.ts\"",
    "lbt": "pnpm run lint && pnpm run build && pnpm run test",
    "lint": "eslint --ext .ts src/",
    "lint:fix": "pnpm run lint -- --fix",
    "test": "jest --passWithNoTests",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:watch": "jest --watch"
  },
  "prettier": "@4s1/eslint-config",
  "dependencies": {
    "base64url": "3.0.1",
    "pino": "7.5.1"
  },
  "devDependencies": {
    "@4s1/eslint-config": "^3.2.0",
    "@types/jest": "27.0.3",
    "@types/node": "14.18.0",
    "@types/pino": "7.0.4",
    "eslint": "8.4.1",
    "jest": "27.4.3",
    "prettier": "2.5.1",
    "ts-jest": "27.1.0",
    "ts-node": "10.4.0",
    "typescript": "4.5.2"
  },
  "engines": {
    "node": ">=14"
  }
}
