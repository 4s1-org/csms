#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/steffen/Dokumente/GIT/csms/node_modules/.pnpm/react-scripts@4.0.2_react@17.0.1+typescript@4.1.3/node_modules/react-scripts/bin/react-scripts.js/node_modules:/home/steffen/Dokumente/GIT/csms/node_modules/.pnpm/react-scripts@4.0.2_react@17.0.1+typescript@4.1.3/node_modules/react-scripts/bin/node_modules:/home/steffen/Dokumente/GIT/csms/node_modules/.pnpm/react-scripts@4.0.2_react@17.0.1+typescript@4.1.3/node_modules/react-scripts/node_modules:/home/steffen/Dokumente/GIT/csms/node_modules/.pnpm/react-scripts@4.0.2_react@17.0.1+typescript@4.1.3/node_modules:/home/steffen/Dokumente/GIT/csms/node_modules/.pnpm/node_modules:/home/steffen/Dokumente/GIT/csms/node_modules:/home/steffen/Dokumente/GIT/node_modules:/home/steffen/Dokumente/node_modules:/home/steffen/node_modules:/home/node_modules:/node_modules:/home/steffen/Dokumente/GIT/csms/backend-admin-ui/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/react-scripts@4.0.2_react@17.0.1+typescript@4.1.3/node_modules/react-scripts/bin/react-scripts.js" "$@"
else 
  exec node  "$basedir/../../../node_modules/.pnpm/react-scripts@4.0.2_react@17.0.1+typescript@4.1.3/node_modules/react-scripts/bin/react-scripts.js" "$@"
fi
