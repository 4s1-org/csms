{
  "name": "@yellowgarbagebag/css-ui",
  "version": "1.0.0",
  "private": true,
  "description": "Charging Station Simulator UI",
  "bugs": {
    "url": "https://gitlab.com/yellow-garbage-group/csms/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:yellow-garbage-group/csms.git"
  },
  "license": "MIT",
  "author": "Steffen <YellowGarbageBag@4s1.de>",
  "scripts": {
    "build": "SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "preinstall": "npx only-allow pnpm",
    "lbt": "npm run lint && npm run build && npm run test",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "start": "PORT=3002 SKIP_PREFLIGHT_CHECK=true react-scripts start",
    "test": "echo No tests"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": "@hest-lab/eslint-config",
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "dependencies": {
    "@testing-library/dom": "^7.31.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@yellowgarbagebag/common-lib": "workspace:*",
    "@yellowgarbagebag/css-lib": "workspace:*",
    "@yellowgarbagebag/ocpp-lib": "workspace:*",
    "bootstrap": "^4.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "typescript": "^4.2.4",
    "web-vitals": "^1.1.2"
  },
  "devDependencies": {
    "@hest-lab/eslint-config": "^1.6.1",
    "@types/node": "^14.17.0",
    "eslint": "^7.26.0",
    "prettier": "^2.3.0"
  },
  "engines": {
    "node": ">=14"
  }
}
