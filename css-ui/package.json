  "scripts": {
    "build": "SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "preinstall": "npx only-allow pnpm",
    "lbt": "npm run lint && npm run build && npm run test",
    "lint": "eslint --ext .ts --ext tsx src/",
    "lint:fix": "npm run lint -- --fix",
    "start": "PORT=3002 SKIP_PREFLIGHT_CHECK=true react-scripts start",
    "test": "echo No tests"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": "@yellowgarbagegroup/eslint-config",
  "dependencies": {
    "@testing-library/dom": "7.31.2",
    "@testing-library/react": "11.2.7",
    "@testing-library/user-event": "13.2.1",
    "@types/react": "17.0.18",
    "@types/react-dom": "17.0.9",
    "@yellowgarbagebag/common-lib": "workspace:*",
    "@yellowgarbagebag/css-lib": "workspace:*",
    "@yellowgarbagebag/ocpp-lib": "workspace:*",
    "bootstrap": "4.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "typescript": "4.3.5",
    "web-vitals": "1.1.2"
  },
  "devDependencies": {
    "@types/node": "^14.17.9",
    "@yellowgarbagegroup/eslint-config": "^2.0.1",
