  "description": "Charge Station Management System - Utils",
  "homepage": "https://gitlab.com/YellowGarbageBag/csms-utils",
  "bugs": {
    "url": "https://gitlab.com/YellowGarbageBag/csms-utils/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com:YellowGarbageBag/csms-utils.git"
  },
  "license": "UNLICENSED",
  "author": "Steffen",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"scripts/**/*.ts\"",
    "lbt": "npm run lint && npm run build && npm run test",
    "lint": "eslint \"{src,scripts}/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "pjs": "ts-node scripts/parse-json-schema/index.ts",
    "pjs:full": "npm run reset && npm run pjs && git status",
    "reset": "rm -rf src/ && git checkout HEAD~1 -- src/",
    "test": "echo No tests available",
    "test:cov": "echo No tests available"
  },
  "prettier": "@hest-lab/eslint-config",
  "dependencies": {},
  "devDependencies": {
    "@hest-lab/eslint-config": "^1.3.0",
    "@nestjs/swagger": "^4.7.8",
    "@types/node": "^14.14.16",
    "class-validator": "^0.12.2",
    "eslint": "^7.16.0",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
